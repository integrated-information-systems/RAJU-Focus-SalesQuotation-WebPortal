<?xml version="1.0"?>
<configuration>

  <location path="Register.aspx">
    <system.web>
      <authorization>
          <allow roles="Admin" />
          <deny users="*"/>
      </authorization>
    </system.web>
  </location>

    <location path="ManageRoles.aspx">
        <system.web>
            <authorization>
                <allow roles="Admin" />
                <deny users="*"/>
            </authorization>
        </system.web>
    </location>

    <system.web>
    <authorization>
      <deny users="?"/>
        <allow  users="admin"/>
    </authorization>
  </system.web>

</configuration>
